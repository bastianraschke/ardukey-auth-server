        <record model="ir.ui.view" id="act_translation_update_init_view_form">
            <field name="model">ir.translation.update.init</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <![CDATA[
                <form string="Synchronize Translations">
                    <label name="lang"/>
                    <field name="lang"/>
                </form>
                ]]>
